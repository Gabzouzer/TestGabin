// Declarative //
pipeline {
	agent any
	stages {
		stage('Build') {
			steps {
				bat '''
                   		 echo "PATH = %PATH%"
                   		 echo "M2_HOME = %M2_HOME%"
				 mvn --version
               			 '''
			}
		}
		stage('Test') {
			steps {
				echo 'Testing..'
			}
		}
		stage('Deploy') {
			steps {
				echo 'Deploying....'
			}
		}
	}
}
